import matplotlib.pyplot as plt

x = [1, 3, 5, 7, 9]
y = [3, 5, 7, 4, 11]
z = [4.5, 6, 4, 7, 9]
plt.subplot(1,2,1)
plt.plot(x, y,"r" ,label='merah')
plt.legend(loc=0)
plt.title ("KPI")
plt.xlabel ('date')
plt.ylabel ('availability')

plt.subplot(1,2,2)
plt.plot(x, z,"b" ,label='biru')

plt.legend(loc=0)
plt.title ("KPI")
plt.xlabel ('date')
plt.ylabel ('availability')

plt.tight_layout()

plt.show()
